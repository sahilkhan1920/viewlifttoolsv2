{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "jsxImportSource": "@emotion/react",
    "incremental": true,
    "baseUrl": ".",
    "alwaysStrict": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/components/MainHeader.jsx",
    "src/components/common/charts/VLBarChart.jsx",
    "src/components/Analytics/RealTime/RealTopContent.jsx",
    "src/components/Analytics/common/RealStreamingPlatforms.jsx",
    "src/hooks/useRealTimeData.js",
    "src/components/Analytics/RealTime/RealStreamingUsersReport.jsx",
    "src/components/Analytics/RealTime/ContentLevelQOS.jsx",
    "src/components/Analytics/Dashboard/UserBehaviourReport.jsx",
    "src/hooks/useHeaders.js",
    "src/helpers/filtersMap.js",
    "src/components/Analytics/Dashboard/TopContentReport.jsx",
    "src/components/Analytics/Dashboard/SubscribersReport.jsx",
    "src/components/Analytics/common/Filters.jsx",
    "src/components/Analytics/common/CommonHeader.jsx",
    "src/components/Analytics/Audience/AudienceOverview.jsx",
    "src/components/common/VLDataviewSwitch.jsx",
    "src/components/Analytics/Dashboard/StreamingDuration.jsx",
    "src/hooks/useGoogleAnalytics.js",
    "src/components/VersionHistory.jsx",
    "pages/contentModels/index.jsx",
    "pages/ui-plansNew/[addOrEditPlan].jsx",
    "src/components/AddNewPlan/FeatureDetails.jsx",
    "src/components/AddNewPlan/MetaData.jsx",
    "src/components/AddNewPlan/PlanDetailsField.jsx",
    "src/components/AddNewPlan/PlanInfo.jsx",
    "src/components/ChoosePlanType.jsx",
    "src/components/ManageAcceptableOffer/ManageAcceptableOffer.jsx",
    "src/components/Modals/AddMonetizationModel/index.jsx",
    "src/components/Panels/MonetizationModelsPanel.jsx",
    "src/components/Panels/PlansNew.jsx",
    "src/components/common/Popup.jsx",
    "src/components/common/VLTableNew.jsx",
    "src/components/common/SortingTable.tsx",
    "src/hooks/useTabs.js",
    "src/contexts/AppContext.tsx",
    "src/components/common/VLTabPanel.jsx"
  ],
  "exclude": ["node_modules"],
  "linterOptions": {
    "exclude": ["src/**", "components/**"]
  }
}
